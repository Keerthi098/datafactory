{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AnviDataFactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"InputBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'InputBlob'"
		},
		"Output_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Output'"
		},
		"Output_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azuredatabas.database.windows.net"
		},
		"Output_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "demodev"
		},
		"Output_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ambi"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AnviDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"InputBlob_connectionString": {
						"value": "[parameters('InputBlob_connectionString')]"
					},
					"Output_password": {
						"value": "[parameters('Output_password')]"
					},
					"Output_properties_typeProperties_server": {
						"value": "[parameters('Output_properties_typeProperties_server')]"
					},
					"Output_properties_typeProperties_database": {
						"value": "[parameters('Output_properties_typeProperties_database')]"
					},
					"Output_properties_typeProperties_userName": {
						"value": "[parameters('Output_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}